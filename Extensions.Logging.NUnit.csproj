<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>NUnit output logger provider implementation for Microsoft.Extensions.Logging.  This logger logs messages to an NUnit console with NUnit.Framework.TestContext.Progress.WriteLine()</Description>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Marshall Moorman</Authors>
    <Version>1.0.1</Version>
    <PackageLicenseUrl>https://github.com/MarshallMoorman/Extensions.Logging.NUnit/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/MarshallMoorman/Extensions.Logging.NUnit</PackageProjectUrl>
    <RepositoryUrl>https://github.com/MarshallMoorman/Extensions.Logging.NUnit</RepositoryUrl>
    <PackageTags>Microsoft.Extensions.Logging, NUnit</PackageTags>
    
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="2.0.0" />
    <PackageReference Include="NUnit" Version="3.12.0" />
  </ItemGroup>

</Project>
